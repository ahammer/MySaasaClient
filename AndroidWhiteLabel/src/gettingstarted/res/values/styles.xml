        <item name="android:padding">12dp</item>
    </style>

    <style name="mysaasa_blogpost_body">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="mysaasa_blogcomment">
        <item name="android:layout_margin">5dp</item>
