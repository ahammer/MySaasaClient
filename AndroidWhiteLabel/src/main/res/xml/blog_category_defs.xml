<?xml version="1.0" encoding="utf-8"?>
    <!--
    This file will define what shows up on the Home screen, in a whitelist for
    -->
<whitelist xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <title>Blog</title>
        <category>Blog</category>
        <sortColumn>dateCreated</sortColumn>
        <sortOrder>DESC</sortOrder>
        <pagesize>100</pagesize>
        <commentsAllowed>true</commentsAllowed>
        <postsAllowed>false</postsAllowed>
    </item>

    <item>
        <title>News</title>
        <category>SocialNewsGeneral</category>
        <sortColumn>dateCreated</sortColumn>
        <sortOrder>DESC</sortOrder>
        <pagesize>100</pagesize>
        <commentsAllowed>true</commentsAllowed>
        <postsAllowed>true</postsAllowed>
    </item>

    <item>
        <title>Contact</title>
        <category>Contact</category>
        <fragment>com.mysaasa.ui.fragments.ContactsFragment</fragment>
    </item>
    <item>
        <title>Messages</title>
        <category>Messages</category>
        <fragment>com.mysaasa.ui.fragments.MessagesFragment</fragment>
    </item>
</whitelist>